<Window x:Name="MainWindow1" x:Class="FaviconBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FaviconBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="900">
    <Grid>
        <Button x:Name="btnDownload" Content="Download Favicon" HorizontalAlignment="Left" Margin="23,15,0,0" VerticalAlignment="Top" Width="163" RenderTransformOrigin="-0.482,-2.142" Click="btnDownload_Click"/>
        <WrapPanel x:Name="m_WrapPanel" HorizontalAlignment="Left" Height="228" Margin="23,51,0,0" VerticalAlignment="Top" Width="850"/>
        <Button x:Name="btnEAP" Content="EAP Download" HorizontalAlignment="Left" Margin="201,15,0,0" VerticalAlignment="Top" Width="93" Click="btnEAP_Click"/>
        <Button x:Name="btnAsync" Content="Async Download" HorizontalAlignment="Left" Margin="309,15,0,0" VerticalAlignment="Top" Width="109" Click="btnAsync_Click"/>
        <Button x:Name="btnExceptionTest" Content="ExceptionTest" HorizontalAlignment="Left" Margin="441,15,0,0" VerticalAlignment="Top" Width="95" Click="btnExceptionTest_Click"/>
    </Grid>
</Window>
